            android:theme="@style/Theme.AppCompat.AddPopUp" />
        <activity android:name=".Registracija">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
